{
  "Trial": {
    "nctId": "protocolSection.identificationModule.nctId",
    "orgStudyId": "protocolSection.identificationModule.orgStudyIdInfo.id",
    "briefTitle": "protocolSection.identificationModule.briefTitle",
    "officialTitle": "protocolSection.identificationModule.officialTitle",
    "acronym": "protocolSection.identificationModule.acronym",
    "overallStatus": "protocolSection.statusModule.overallStatus",
    "statusVerifiedDate": "protocolSection.statusModule.statusVerifiedDate",
    "startDate": "protocolSection.statusModule.startDateStruct.date",
    "startDateType": "protocolSection.statusModule.startDateStruct.type",
    "primaryCompletionDate": "protocolSection.statusModule.primaryCompletionDateStruct.date",
    "primaryCompletionDateType": "protocolSection.statusModule.primaryCompletionDateStruct.type",
    "completionDate": "protocolSection.statusModule.completionDateStruct.date",
    "completionDateType": "protocolSection.statusModule.completionDateStruct.type",
    "studyFirstSubmitDate": "protocolSection.statusModule.studyFirstSubmitDate",
    "studyFirstPostDate": "protocolSection.statusModule.studyFirstPostDateStruct.date",
    "lastUpdatePostDate": "protocolSection.statusModule.lastUpdatePostDateStruct.date",
    "studyType": "protocolSection.designModule.studyType",
    "phases": "protocolSection.designModule.phases",
    "interventionModel": "protocolSection.designModule.designInfo.interventionModel",
    "allocation": "protocolSection.designModule.designInfo.allocation",
    "primaryPurpose": "protocolSection.designModule.designInfo.primaryPurpose",
    "masking": "protocolSection.designModule.designInfo.maskingInfo.masking",
    "enrollmentCount": "protocolSection.designModule.enrollmentInfo.count",
    "enrollmentType": "protocolSection.designModule.enrollmentInfo.type",
    "briefSummary": "protocolSection.descriptionModule.briefSummary",
    "detailedDescription": "protocolSection.descriptionModule.detailedDescription",
    "hasResults": "hasResults",
    "ipdSharing": "protocolSection.ipdSharingStatementModule.ipdSharing",
    "versionHolder": "protocolSection.derivedSection.miscInfoModule.versionHolder"
  },
  "Organization": {
    "name": "protocolSection.sponsorCollaboratorsModule.leadSponsor.name",
    "class": "protocolSection.sponsorCollaboratorsModule.leadSponsor.class",
    "aliases": [
      "protocolSection.sponsorCollaboratorsModule.responsibleParty.oldOrganization",
      "protocolSection.identificationModule.organization.fullName"
    ]
  },
  "Condition": {
    "name": "protocolSection.conditionsModule.conditions[]",
    "keywords": "protocolSection.conditionsModule.keywords[]",
    "meshIds": "protocolSection.derivedSection.conditionBrowseModule.meshes[].id",
    "ancestors": "protocolSection.derivedSection.conditionBrowseModule.ancestors[].term"
  },
  "Intervention": {
    "name": "protocolSection.armsInterventionsModule.interventions[].name",
    "type": "protocolSection.armsInterventionsModule.interventions[].type",
    "description": "protocolSection.armsInterventionsModule.interventions[].description",
    "otherNames": "protocolSection.armsInterventionsModule.interventions[].otherNames",
    "armGroupLabels": "protocolSection.armsInterventionsModule.interventions[].armGroupLabels",
    "meshIds": "protocolSection.derivedSection.interventionBrowseModule.meshes[].id"
  },
  "Arm": {
    "armId": "constructed: nctId + '::' + arm.label",
    "label": "protocolSection.armsInterventionsModule.armGroups[].label",
    "type": "protocolSection.armsInterventionsModule.armGroups[].type",
    "description": "protocolSection.armsInterventionsModule.armGroups[].description",
    "interventionNames": "protocolSection.armsInterventionsModule.armGroups[].interventionNames"
  },
  "Site": {
    "facility": "protocolSection.contactsLocationsModule.locations[].facility",
    "city": "protocolSection.contactsLocationsModule.locations[].city",
    "state": "protocolSection.contactsLocationsModule.locations[].state",
    "zip": "protocolSection.contactsLocationsModule.locations[].zip",
    "country": "protocolSection.contactsLocationsModule.locations[].country",
    "status": "protocolSection.contactsLocationsModule.locations[].status",
    "latitude": "protocolSection.contactsLocationsModule.locations[].geoPoint.lat",
    "longitude": "protocolSection.contactsLocationsModule.locations[].geoPoint.lon"
  },
  "Investigator": {
    "name": "protocolSection.contactsLocationsModule.overallOfficials[].name",
    "affiliation": "protocolSection.contactsLocationsModule.overallOfficials[].affiliation",
    "role": "protocolSection.contactsLocationsModule.overallOfficials[].role",
    "email": "protocolSection.contactsLocationsModule.centralContacts[].email",
    "phone": "protocolSection.contactsLocationsModule.centralContacts[].phone"
  },
  "Contact": {
    "name": "protocolSection.contactsLocationsModule.centralContacts[].name",
    "role": "protocolSection.contactsLocationsModule.centralContacts[].role",
    "phone": "protocolSection.contactsLocationsModule.centralContacts[].phone",
    "email": "protocolSection.contactsLocationsModule.centralContacts[].email",
    "organization": "protocolSection.contactsLocationsModule.centralContacts[].organization"
  },
  "Outcome": {
    "outcomeId": "constructed: nctId + '::' + measure hash",
    "measure": "protocolSection.outcomesModule.*[].measure",
    "description": "protocolSection.outcomesModule.*[].description",
    "timeFrame": "protocolSection.outcomesModule.*[].timeFrame",
    "outcomeType": "PRIMARY|SECONDARY|OTHER (based on array used)"
  },
  "Result": {
    "resultId": "constructed: nctId + '::' + outcomeTitle + '::' + groupId",
    "nctId": "protocolSection.identificationModule.nctId",
    "outcomeTitle": "resultsSection.outcomeMeasuresModule.outcomeMeasures[].title",
    "groupId": "resultsSection.outcomeMeasuresModule.outcomeMeasures[].classes[].categories[].measurements[].groupId",
    "value": "resultsSection.outcomeMeasuresModule.outcomeMeasures[].classes[].categories[].measurements[].value",
    "spread": "resultsSection.outcomeMeasuresModule.outcomeMeasures[].classes[].categories[].measurements[].spread",
    "denominator": "resultsSection.outcomeMeasuresModule.outcomeMeasures[].denoms[]"
  },
  "BaselineGroup": {
    "baselineGroupId": "constructed: nctId + '::BG::' + group.title",
    "title": "resultsSection.baselineCharacteristicsModule.groups[].title",
    "description": "resultsSection.baselineCharacteristicsModule.groups[].description"
  },
  "BaselineMeasure": {
    "baselineMeasureId": "constructed: nctId + '::BM::' + measure.title",
    "title": "resultsSection.baselineCharacteristicsModule.measures[].title",
    "paramType": "resultsSection.baselineCharacteristicsModule.measures[].paramType",
    "unitOfMeasure": "resultsSection.baselineCharacteristicsModule.measures[].unitOfMeasure",
    "raw": "store full measure JSON for provenance"
  },
  "ParticipantFlowGroup": {
    "flowGroupId": "constructed: nctId + '::PF::' + group.id",
    "title": "resultsSection.participantFlowModule.groups[].title",
    "description": "resultsSection.participantFlowModule.groups[].description"
  },
  "FlowAchievement": {
    "flowAchievementId": "constructed",
    "flowGroupId": "resultsSection.participantFlowModule.periods[].milestones[].achievements[].groupId",
    "periodTitle": "resultsSection.participantFlowModule.periods[].title",
    "type": "resultsSection.participantFlowModule.periods[].milestones[].type",
    "numSubjects": "resultsSection.participantFlowModule.periods[].milestones[].achievements[].numSubjects"
  },
  "AdverseEvent": {
    "adEventId": "constructed: nctId + '::AE::' + term",
    "term": "resultsSection.adverseEventsModule.seriousEvents[].term",
    "organSystem": "resultsSection.adverseEventsModule.seriousEvents[].organSystem",
    "assessmentType": "resultsSection.adverseEventsModule.seriousEvents[].assessmentType",
    "notes": "resultsSection.adverseEventsModule.seriousEvents[].notes"
  },
  "AdverseEventStat": {
    "adEventStatId": "constructed",
    "adEventId": "see AdverseEvent.adEventId",
    "groupId": "resultsSection.adverseEventsModule.seriousEvents[].stats[].groupId",
    "numEvents": "resultsSection.adverseEventsModule.seriousEvents[].stats[].numEvents",
    "numAffected": "resultsSection.adverseEventsModule.seriousEvents[].stats[].numAffected",
    "numAtRisk": "resultsSection.adverseEventsModule.seriousEvents[].stats[].numAtRisk"
  },
  "EligibilityCriterion": {
    "criterionId": "constructed: nctId + '::IN|EX::#' + seq",
    "nctId": "protocolSection.identificationModule.nctId",
    "type": "INCLUSION|EXCLUSION (derived by splitting eligibility text)",
    "text": "protocolSection.eligibilityModule.eligibilityCriteria",
    "sequence": "derived integer ordering"
  },
  "Publication": {
    "publicationId": "protocolSection.referencesModule.references[].pmid OR constructed id",
    "pmid": "protocolSection.referencesModule.references[].pmid",
    "citation": "protocolSection.referencesModule.references[].citation",
    "type": "protocolSection.referencesModule.references[].type"
  },
  "Version": {
    "versionId": "constructed import id or derived from versionHolder",
    "versionHolder": "protocolSection.derivedSection.miscInfoModule.versionHolder",
    "importDate": "ETL timestamp"
  },
  "VectorChunk (omitted now)": {
    "chunkId": "constructed",
    "nctId": "protocolSection.identificationModule.nctId",
    "field": "briefSummary|detailedDescription|eligibility|outcomes",
    "offset": "chunk offset",
    "text": "text chunk (derived)",
    "lengthChars": "derived"
  },
  "Relationships": {
    "trial_sponsoredby": {
      "from": "Trial.nctId",
      "to": "Organization.name",
      "props": {"role": "lead_sponsor"}
    },
    "trial_studies": {
      "from": "Trial.nctId",
      "to": "Condition.name",
      "props": {}
    },
    "trial_uses_intervention": {
      "from": "Trial.nctId",
      "to": "Intervention.name",
      "props": {}
    },
    "trial_has_arm": {
      "from": "Trial.nctId",
      "to": "Arm.armId",
      "props": {}
    },
    "arm_contains_intervention": {
      "from": "Arm.armId",
      "to": "Intervention.name",
      "props": {}
    },
    "trial_has_site": {
      "from": "Trial.nctId",
      "to": "Site.facility|city|country",
      "props": {"status": "site.status"}
    },
    "trial_has_contact": {
      "from": "Trial.nctId",
      "to": "Contact.name",
      "props": {}
    },
    "trial_has_investigator": {
      "from": "Trial.nctId",
      "to": "Investigator.name",
      "props": {}
    },
    "trial_has_outcome": {
      "from": "Trial.nctId",
      "to": "Outcome.outcomeId",
      "props": {"outcomeType": "PRIMARY|SECONDARY|OTHER"}
    },
    "outcome_has_result": {
      "from": "Outcome.outcomeId",
      "to": "Result.resultId",
      "props": {}
    },
    "trial_has_version": {
      "from": "Trial.nctId",
      "to": "Version.versionId",
      "props": {}
    }
  }
}
